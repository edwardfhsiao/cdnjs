    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "empty-module": "^0.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.7.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "identity-obj-proxy": "^3.0.0",
    "inline-style-prefix-all": "^2.0.2",
